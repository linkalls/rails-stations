<%= form_with(model: @reservation, url: admin_reservations_path, method: :post) do |f| %>
  <%= f.hidden_field :data, value: Date.today.to_s %>
  <%= f.hidden_field :screen_id, value: @schedule.screen.name  %>
  <%#= f.label :name %>
  <%#= f.text_field :name %>
  <%#= f.label :email %>
  <%#= f.text_field :email %>        
  <%= f.label :schedule_id %>
  <%= f.text_field :schedule_id %>
  <%= f.label :sheet_id %>
  <%= f.text_field :sheet_id %>
  <%= f.submit "送信" %>
<% end %>
